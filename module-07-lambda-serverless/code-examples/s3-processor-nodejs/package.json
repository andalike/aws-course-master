{
  "name": "s3-processor-nodejs",
  "version": "1.0.0",
  "description": "AWS Lambda function to process S3 events and uploaded files",
  "main": "index.js",
  "scripts": {
    "test": "node index.js",
    "package": "zip -r function.zip index.js node_modules package.json",
    "deploy": "aws lambda update-function-code --function-name s3-processor-nodejs --zip-file fileb://function.zip"
  },
  "keywords": [
    "aws",
    "lambda",
    "s3",
    "serverless",
    "file-processing"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.400.0",
    "@aws-sdk/client-s3": "^3.400.0",
    "@aws-sdk/util-dynamodb": "^3.400.0"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=18.0.0"
  }
}
